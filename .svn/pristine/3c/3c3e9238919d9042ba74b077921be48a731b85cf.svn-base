/**
OVERRIDE
*/
/* Font-Face */
*{font-family: "儷黑 Pro","LiHei Pro",Meiryo,"微軟正黑體","Microsoft JhengHei",sans-serif;}

/* PreLoader Start */
#loader-wrapper {
    position: fixed;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    z-index: 1000;
}
#loader {
    display: block;
    position: relative;
    left: 50%;
    top: 50%;
    width: 150px;
    height: 150px;
    margin: -75px 0 0 -75px;
    border-radius: 50%;
    border: 3px solid transparent;
    border-top-color: #3498db;
    -webkit-animation: spin 2s linear infinite; /* Chrome, Opera 15+, Safari 5+ */
    animation: spin 2s linear infinite; /* Chrome, Firefox 16+, IE 10+, Opera */
    z-index: 1001;
}

#loader:before {
    content: "";
    position: absolute;
    top: 5px;
    left: 5px;
    right: 5px;
    bottom: 5px;
    border-radius: 50%;
    border: 3px solid transparent;
    border-top-color: #e74c3c;
    -webkit-animation: spin 3s linear infinite; /* Chrome, Opera 15+, Safari 5+ */
    animation: spin 3s linear infinite; /* Chrome, Firefox 16+, IE 10+, Opera */
}

#loader:after {
    content: "";
    position: absolute;
    top: 15px;
    left: 15px;
    right: 15px;
    bottom: 15px;
    border-radius: 50%;
    border: 3px solid transparent;
    border-top-color: #f9c922;
    -webkit-animation: spin 1.5s linear infinite; /* Chrome, Opera 15+, Safari 5+ */
            animation: spin 1.5s linear infinite; /* Chrome, Firefox 16+, IE 10+, Opera */
}

@-webkit-keyframes spin {
    0%   {
        -webkit-transform: rotate(0deg);  /* Chrome, Opera 15+, Safari 3.1+ */
        -ms-transform: rotate(0deg);  /* IE 9 */
        transform: rotate(0deg);  /* Firefox 16+, IE 10+, Opera */
    }
    100% {
        -webkit-transform: rotate(360deg);  /* Chrome, Opera 15+, Safari 3.1+ */
        -ms-transform: rotate(360deg);  /* IE 9 */
        transform: rotate(360deg);  /* Firefox 16+, IE 10+, Opera */
    }
}
@keyframes spin {
    0%   {
        -webkit-transform: rotate(0deg);  /* Chrome, Opera 15+, Safari 3.1+ */
        -ms-transform: rotate(0deg);  /* IE 9 */
        transform: rotate(0deg);  /* Firefox 16+, IE 10+, Opera */
    }
    100% {
        -webkit-transform: rotate(360deg);  /* Chrome, Opera 15+, Safari 3.1+ */
        -ms-transform: rotate(360deg);  /* IE 9 */
        transform: rotate(360deg);  /* Firefox 16+, IE 10+, Opera */
    }
}
#loader-wrapper .loader-section {
    position: fixed;
    top: 0;
    width: 51%;
    height: 100%;
    background: #222222;
    z-index: 1000;
    -webkit-transform: translateX(0);  /* Chrome, Opera 15+, Safari 3.1+ */
    -ms-transform: translateX(0);  /* IE 9 */
    transform: translateX(0);  /* Firefox 16+, IE 10+, Opera */
}
#loader-wrapper .loader-section.section-left {
    left: 0;
}
#loader-wrapper .loader-section.section-right {
    right: 0;
}
/* Loaded */
.loaded #loader-wrapper .loader-section.section-left {
    -webkit-transform: translateX(-100%);  /* Chrome, Opera 15+, Safari 3.1+ */
        -ms-transform: translateX(-100%);  /* IE 9 */
            transform: translateX(-100%);  /* Firefox 16+, IE 10+, Opera */
    -webkit-transition: all 0.7s 0.3s cubic-bezier(0.645, 0.045, 0.355, 1.000);
            transition: all 0.7s 0.3s cubic-bezier(0.645, 0.045, 0.355, 1.000);
}
.loaded #loader-wrapper .loader-section.section-right {
    -webkit-transform: translateX(100%);  /* Chrome, Opera 15+, Safari 3.1+ */
        -ms-transform: translateX(100%);  /* IE 9 */
            transform: translateX(100%);  /* Firefox 16+, IE 10+, Opera */
    -webkit-transition: all 0.7s 0.3s cubic-bezier(0.645, 0.045, 0.355, 1.000);
            transition: all 0.7s 0.3s cubic-bezier(0.645, 0.045, 0.355, 1.000);
}
.loaded #loader {
    opacity: 0;
    -webkit-transition: all 0.3s ease-out;
            transition: all 0.3s ease-out;
}
.loaded #loader-wrapper {
    visibility: hidden;
    -webkit-transform: translateY(-100%);  /* Chrome, Opera 15+, Safari 3.1+ */
        -ms-transform: translateY(-100%);  /* IE 9 */
            transform: translateY(-100%);  /* Firefox 16+, IE 10+, Opera */
    -webkit-transition: all 0.3s 1s ease-out;
            transition: all 0.3s 1s ease-out;
}
/* PreLoader - JavaScript Turned Off */
.no-js #loader-wrapper {
    display: none;
}
/* PreLoader End */

/* Animation, Execution */
@keyframes bounce {
    0%, 20%, 50%, 80%, 100%  {
        -webkit-transform: translateY(0);
        -ms-transform: translateY(0);
        transform: translateY(0);
    }
    40% {
      -webkit-transform: translateY(-15px);
      -ms-transform: translateY(-15px);
      transform: translateY(-15px);
    }
    60% {
      -webkit-transform: translateY(-5px);
      -ms-transform: translateY(-5px);
      transform: translateY(-5px);
    }
}
.bounce {
  animation: bounce 2s infinite;
}
body {
  max-height:100%;
  overflow: hidden;
}
#content-wrapper{
    margin-left: 0 !important;
}
/* Slider */
.jssora22l, .jssora22r {
    display: block;
    position: absolute;
    /* size of arrow element */
    cursor: pointer;
    overflow: hidden;
}
.jssora22l {display: none}
.jssora22r {bottom:0.2em}
.jssora22r i {
    color: white;
    font-size: 3em;
}


/* HEADER */
.section-block {
  position: absolute;
  top: 0;
  height: 2.2em;
  transition: top 0.2s;
  z-index: 2;
  width: 100%;
}
.section-block > div {
  width: 100%;
  margin: 0 auto;
  color: white;
  text-align: center;
  line-height: 2.2em;
  text-transform: uppercase;
  font-weight: 800;
}
.section-block.hide{
  top:-2.2em;
}
.orange {
  background-color: rgb(255,100,25);
}
.blue {
  background-color: rgb(80,125,190);
}
.btn-home {
  border-radius: 50%;
  -o-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  width: 3.8em;
  height: 3.8em;
  position: absolute;
  right: 1.2em;
  bottom: 1.2em;
  cursor: pointer;
}
.btn-home i {
  color: white;
  position: absolute;
  line-height: 1.9em;
  text-align: center;
  width: 1.9em;
  font-size: 2em;
}
/* Main Navigation - Home Btn */
.main-nav {
  background-color: rgba(0,0,0,.9);
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  color: white;
  z-index: 999;
}
.main-nav #floater { float:left; height:50%; margin-bottom:-14em;
    width:1px; /* only for IE7 */
}
.main-nav #middle {  clear:both; height:28em; position:relative; }
.main-nav h2 {
    text-align: center;
    text-transform: uppercase;
    color: white;
    font-weight: 900;
    font-size: 1.2em;
    letter-spacing: 0.2em;
}
.mui-row {
    margin: 0 2em;
}
.key-text {
    position: absolute;
    z-index: 20;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    padding: 0.8em 0.5em;
}
.key-text i {
    display: block;
    text-align: center;
    font-size: 3em;
}
.key-text span {
  font-size: 0.6em;
  text-transform: uppercase;
  line-height: 1.2em;
  text-align: center;
  display: block;
  font-weight: 800;
  width: 80%;
  margin: 0 auto;
}
.main-nav {
  transition: top 0.5s, opacity 0.5s;
}
.main-nav.hide {
  top: 100%;
  opacity: 0;
}
.main-nav .mui-row > div {
    position: relative;
    margin: 1%;
    padding: 0;
    width: calc(33.33333% - 2%);
}
.key-background {
  position: relative;
  z-index: 10;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  background-color: black;
}
.key-background img{
  width:100%;
  height:auto;
  opacity: 0.4;
}

.btn-menu-out {
  border-radius: 50%;
  -o-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  width: 3.8em;
  height: 3.8em;
  position: absolute;
  right: 0.8em;
  bottom: 0.8em;
  cursor: pointer;
}
.btn-menu-out i {
  color: white;
  position: absolute;
  line-height: 1.9em;
  text-align: center;
  width: 1.9em;
  font-size: 2em;
}
